<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.modelink.reservation.mapper.FlowReserveMapper" >

    <select id="findListWithLimitColumnByDateRange"
            parameterType="com.modelink.admin.vo.DashboardParamVo"
            resultType="com.modelink.reservation.bean.FlowReserve">
        <![CDATA[
            select date, reserve_mobile from flow_reserve
            where date >= #{startDate}
            and date =< #{endDate}
            and merchantId = #{merchantId}
        ]]>
    </select>

</mapper>